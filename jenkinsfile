pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('build') {
            steps {
                echo 'build job'
            }
        }
        
        stage('test') {
            steps {
                echo 'testing job'
            }
        }
       stage('deploy') {
            steps {
                echo 'deploy job for downloading application'
                bat 'python  main/reverseval.py'
            }
        } 
    }
}

    

